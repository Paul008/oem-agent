    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260101.0",
    "dotenv": "^17.3.1",
    "puppeteer": "^24.37.2",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.64.0"
  }
